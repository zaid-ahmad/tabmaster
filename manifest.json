{
    "name": "TabMaster",
    "version": "0.0.1",
    "description": "Master your tabs, master your productivity.",
    "permissions": ["storage", "tabs"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_icon": {
        "16": "assets/logo-16.png",
        "24": "assets/logo-24.png",
        "32": "assets/logo-32.png"
      },
      "default_title": "TabMaster",
      "default_popup": "popup.html"
    },
    "manifest_version": 3
}
